        "# QUESTION 2\n",
        "***Create a function that accepts the raw data after it has been loaded and returns the cleaned-up data. ***"
      ],
      "metadata": {
        "id": "ncaf8KH-pCWd"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.preprocessing import StandardScaler"
      ],
      "metadata": {
        "id": "3YoNeI-pohJt"
      },
      "execution_count": 32,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def clean_data(raw_data):\n",
        "    # Handle missing values\n",
        "    cleaned_data = data_df.fillna(0)\n",
        "\n",
        "    # Detect and handle outliers\n",
        "\n",
        "    z_scores = (cleaned_data - cleaned_data.mean()) / cleaned_data.std()\n",
        "    cleaned_data = cleaned_data[(z_scores.abs() < 3).all(axis=1)]\n",
        "\n",
        "    # Perform feature scaling\n",
        "    scaler = StandardScaler()\n",
        "    numerical_columns = cleaned_data.select_dtypes(include=['float64', 'int64']).columns\n",
        "    cleaned_data[numerical_columns] = scaler.fit_transform(cleaned_data[numerical_columns])\n",
        "\n",
        "    return cleaned_data"
      ],
      "metadata": {
        "id": "hODEZYmYl4_y"
      },
      "execution_count": 33,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "datadf =clean_data(data_df)\n",
        "#view clean dataset\n",
        "datadf.head()"
